    "name": "Tomer Figenblat",
    "email": "tomer.figenblat@gmail.com",
    "url": "https://github.com/TomerFi"
  },
  "contributors": [],
  "engines": {},
  "files": [
    ".remarkrc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TomerFi/slackin_docker.git"
  },
  "scripts": {
    "lint-md": "remark --rc-path .remarkrc --frail . .github",
    "validate-pkg": "pjv --filenmae package.json --spec npm --warnings --recommendations",
    "spell-md-interactive": "mdspell --en-us --ignore-numbers --ignore-acronyms '*.md' '.github/**/*.md'",
    "spell-md-report": "mdspell --en-us --ignore-numbers --ignore-acronyms --report '*.md' '.github/**/*.md'"
  },
  "dependencies": {
    "lodash": "4.17.15",
    "markdown-spellcheck": "1.3.1",
    "marked": "0.8.0",
    "package-json-validator": "0.6.3",
    "remark-cli": "7.0.1",
    "remark-frontmatter": "1.3.2",
    "remark-lint": "6.0.5",
    "remark-preset-lint-consistent": "2.0.3",
    "remark-preset-lint-markdown-style-guide": "2.1.3",
    "remark-preset-lint-recommended": "3.0.3"
  },
  "private": true
}
