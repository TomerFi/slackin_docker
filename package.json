{
  "name": "slackin_docker",
  "version": "0.5.1",
  "description": "Docker image for hosting rauchg's solution for slack channel badge.",
  "keywords": [
    "slack",
    "slackin",
    "badge",
    "docker"
  ],
  "homepage": "https://github.com/TomerFi/slackin_docker/",
